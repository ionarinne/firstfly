#include<stdio.h>
int main() {
    int a, b, c; // 声明三个整数变量
    printf("输入三个个整数:");
    scanf_s("%d %d %d", &a, &b, &c); // 从用户读取三个整数

    int max = 0;
    if (a > b) {
        if (a > c) {
            max = a;
        }
        else {
            max = c;
        }
    }
    else {
        if (b > c) {
            max = b;
        }
        else {
            max = c;
        }
    }
    printf("%d", max);
    return 0; // 返回程序执行成功的信号
}
